#include <bits/stdc++.h>
#include <iostream>
#include <utility>
using namespace std;

class Node{

public:
       int data;
       Node* next;
       Node* back;

       Node(int data1, Node* next1, Node* back1){
        data = data1;
        next = next1;
        back = back1;
       }

       Node(int val){                         // Intial condition where both pointers are NULL;
        data = val;
        next = NULL;
        back = NULL;
       }


};

Node* ConvertDLL(vector <int> &arr){
    Node* head = new Node(arr[0]);
    Node* prev = head;
    for(int i = 1; i< arr.size(); i++){
        Node* temp = new Node(arr[i], NULL,prev);
        prev->next = temp;
        prev = temp;

    }
    return head;
}


void print(Node* head){
    Node* temp = head;
    while(temp != NULL){
        cout<<temp->data<<" ";
        temp = temp->next;
    }

}


int main(){

    vector <int> arr = {1,2,3,4,5};
    Node* head = ConvertDLL(arr);
    print(head);
    
    
    return 0;
}
